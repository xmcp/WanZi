{
  "name": "丸子",
  "version": "1.0",
  "description": "人大附中游戏必备",
  "icons": {"128": "icons/icon.png"},

  "manifest_version": 2,
  "minimum_chrome_version" : "17.0.0",

  "permissions": [
    "tabs",
    "*://*/*",
    "webRequestBlocking",
    "webRequest"
  ],

  "options_page": "options.html",
  "background": {
    "scripts": ["background.js"]
  },
  "page_action": {
    "default_icon": {"38": "icons/page_icon.png"},
    "default_title": "丸子大法好",
    "default_popup": "popup.html"
  }
}